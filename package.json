    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*",
      "public/**/*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
